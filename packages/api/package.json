{
  "name": "api",
  "private": true,
  "version": "0.0.0",
  "description": "fleet management software - api",
  "author": "Mohammed Hamdoune <sidlynx@gmail.com>",
  "license": "UNLICENSED",
  "type": "commonjs",
  "keywords": [],
  "scripts": {
    "dev": "pnpm run start:dev",
    "build": "nest build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint-fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-watch-all": "jest --watchAll",
    "clean": "pnpm run clean-install && pnpm run clean-build",
    "clean-install": "pnpm dlx rimraf node_modules package-lock.json",
    "clean-build": "pnpm dlx rimraf dist *.tsbuildinfo",
    "----------": "----------",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.2",
    "@nestjs/core": "^10.4.4",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/serve-static": "^4.0.2",
    "@trpc/server": "^10.45.2",
    "@types/config": "^3.3.5",
    "config": "^3.3.12",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "toml": "^3.0.0",
    "trpc": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.2",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "^20.18.0",
    "pnpm": "9.12.2"
  }
}
